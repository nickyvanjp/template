    "three": "^0.106.2"
  },
  "devDependencies": {
    "glslify": "^7.0.0",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^2.0.0",
    "raw-loader": "^3.1.0",
    "ts-loader": "^6.1.2",
    "typescript": "^3.6.3"
  }
}
