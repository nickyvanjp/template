{
  "name": "frontend-template",
  "version": "1.0.0",
  "description": "template",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://backlog.caters.jp:8969/backlog/git/FRONTEND/FrontEndTemplate-v2.git"
  },
  "browserslist": [
    "> 5% in alt-AS",
    "last 2 major versions",
    "iOS >= 9",
    "ie 11"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": "off"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "scripts": {
    "start": "yarn run-p clean:* & yarn run-p watch:*",
    "clean:host": "lsof -n -i:3000 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "clean:js": "yarn run rimraf \"../public_html/assets/js\"",
    "clean:css": "yarn run rimraf \"../public_html/assets/css\"",
    "watch:pug": "yarn pug src/pug/ --hierarchy -o ../public_html/ -P -w",
    "watch:css": "postcss \"src/css/**/!(_)*.css\" -d \"../public_html/assets/css\" -c \"./postcss.config.js\" --env=development --ext css -w",
    "watch:image": "node \"npm_scripts/watch_image\"",
    "build:image": "node \"npm_scripts/cpx_image\"",
    "watch:icon": "chokidar \"src/fonts/**/*.svg\" -c \"yarn build:icon\"",
    "build:icon": "mkdir \"../public_html/assets/fonts\" & icon-font-generator \"./src/fonts/*.svg\" -o \"../public_html/assets/fonts/\" --csspath \"src/css/_config/_fonts.css\" -p glyphs --name glyphs --json false --mono true --fontspath \"/assets/fonts\"",
    "watch:js": "webpack -w --env.NODE_ENV=development",
    "build": "webpack --env.NODE_ENV=production",
    "watch:localhost": "yarn browser-sync start --config bs-config.js",
    "esdoc": "esdoc"
  },
  "author": "matsumoto",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@svgr/webpack": "^4.0.3",
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "browser-sync": "^2.26.3",
    "chokidar-cli": "^1.2.1",
    "cpx": "^1.5.0",
    "css-declaration-sorter": "^4.0.1",
    "css-mqpacker": "^7.0.0",
    "csswring": "^7.0.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-node": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "event-hooks-webpack-plugin": "^2.1.1",
    "fetch-jsonp": "^1.1.3",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.86.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "^0.0.1-security",
    "gsap": "^3.6.0",
    "icon-font-generator": "^2.1.8",
    "image-size": "^0.6.3",
    "intersection-observer": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "picturefill": "^3.0.3",
    "postcss": "^7.0.5",
    "postcss-cachebuster": "^0.1.5",
    "postcss-center": "^1.1.0",
    "postcss-cli": "^6.0.1",
    "postcss-comment": "^2.0.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-extend-rule": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-for": "^2.1.1",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-normalize": "^7.0.1",
    "postcss-reporter": "^6.0.0",
    "postcss-simple-vars": "^5.0.1",
    "postcss-will-change-transition": "^1.2.0",
    "pug-cli": "https://github.com/pugjs/pug-cli.git",
    "react": "^16.6.3",
